{
  "name": "alexbuijsweb-spa",
  "version": "1.0.0",
  "description": "SPA for alexbuijsweb",
  "author": "Alex Buijs <contact@alexbuijsweb.nl>",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "lint": "eslint --ext .js,.vue src",
    "build": "node build/build.js",
    "sync_assets": "aws s3 sync dist s3://www.alexbuijsweb.nl --acl public-read --cache-control max-age=31536000 --delete --size-only --exclude index.html",
    "sync_index": "aws s3 sync dist s3://www.alexbuijsweb.nl --acl public-read --cache-control max-age=60 --exclude '*' --include index.html",
    "deploy": "yarn build && yarn sync_assets && yarn sync_index"
  },
  "dependencies": {
    "bootstrap-vue": "0.16.1",
    "font-awesome": "4.7.0",
    "jqcloud2": "2.0.3",
    "jquery": "3.2.1",
    "vue": "2.3.4",
    "vue-analytics": "4.1.2",
    "vue-router": "2.5.3",
    "vue2-google-maps": "0.7.9"
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.5.2",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "chalk": "1.1.3",
    "compression-webpack-plugin": "0.4.0",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.4",
    "eslint": "4.0.0",
    "eslint-config-standard": "10.2.1",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.8.0",
    "eslint-plugin-html": "3.0.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "eventsource-polyfill": "0.9.6",
    "express": "4.15.3",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "friendly-errors-webpack-plugin": "1.6.1",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "0.17.4",
    "node-sass": "4.5.3",
    "opn": "5.1.0",
    "optimize-css-assets-webpack-plugin": "2.0.0",
    "ora": "1.3.0",
    "pug": "2.0.0-rc.2",
    "pug-loader": "2.3.0",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "semver": "5.3.0",
    "standard": "10.0.2",
    "url-loader": "0.5.9",
    "vue-loader": "12.2.1",
    "vue-style-loader": "3.0.1",
    "vue-template-compiler": "2.3.4",
    "webpack": "2.6.1",
    "webpack-bundle-analyzer": "2.8.2",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0",
    "webpack-merge": "4.1.0"
  },
  "engines": {
    "node": ">= 8.0.0",
    "yarn": ">= 0.24.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
